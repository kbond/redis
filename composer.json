{
    "name": "zenstruck/redis",
    "description": "Lazy proxy for php-redis with DX helpers and utilities.",
    "homepage": "https://github.com/zenstruck/redis",
    "type": "library",
    "license": "MIT",
    "keywords": ["redis"],
    "authors": [
        {
            "name": "Kevin Bond",
            "email": "kevinbond@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-redis": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^9.5.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/var-dumper": "^4.4|^5.0|^6.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": { "Zenstruck\\": ["src/"] }
    },
    "autoload-dev": {
        "psr-4": { "Zenstruck\\Redis\\Tests\\": ["tests/"] }
    }
}
